{
  "name": "@netko/claw",
  "version": "0.2.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run dev:setup && bun run dev:serve",
    "dev:serve": "bunx --bun vite dev --port 3000",
    "start": "bun .output/server/index.mjs",
    "build": "NODE_ENV=production vite build",
    "serve": "bunx --bun vite preview",
    "prod:release": "NODE_ENV=production bun run prod:setup && bun run start",
    "prod:setup": "bun run db:generate && bun run db:deploy && bun run prod:db:seed",
    "dev:setup": "bun run db:generate && bun run db:migrate && bun run dev:db:seed",
    "prod:db:seed": "bun run --filter @netko/claw-repository db:seed",
    "dev:db:seed": "bun run --env-file=../../../apps/claw/.env --cwd ../../packages/claw/repository db:seed",
    "db:migrate": "bun run --env-file=../../../apps/claw/.env --cwd ../../packages/claw/repository db:migrate",
    "db:generate": "bun run --filter @netko/claw-repository db:generate",
    "db:deploy": "bun run --filter @netko/claw-repository db:deploy"
  },
  "dependencies": {
    "@ai-sdk/react": "^1.2.12",
    "@netko/claw-config": "workspace:*",
    "@netko/claw-domain": "workspace:*",
    "@netko/claw-repository": "workspace:*",
    "@netko/claw-service": "workspace:*",
    "@netko/claw-trpc": "workspace:*",
    "@netko/logger": "workspace:*",
    "@netko/ui": "workspace:*",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "@remixicon/react": "^4.6.0",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-devtools": "^0.7.11",
    "@tanstack/react-query": "^5.66.5",
    "@tanstack/react-query-devtools": "^5.84.2",
    "@tanstack/react-router": "^1.132.0",
    "@tanstack/react-router-devtools": "^1.132.0",
    "@tanstack/react-router-ssr-query": "^1.131.7",
    "@tanstack/react-start": "^1.132.24",
    "@tanstack/router-plugin": "^1.132.0",
    "@trpc/client": "^11.4.3",
    "@trpc/server": "^11.4.3",
    "@trpc/tanstack-react-query": "^11.4.3",
    "@xyflow/react": "^12.10.0",
    "ai": "^5.0.0-canary.14",
    "better-auth": "^1.3.23",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "embla-carousel-react": "^8.6.0",
    "lucide-react": "^0.556.0",
    "motion": "^12.23.25",
    "nanoid": "^5.1.6",
    "nitro": "npm:nitro-nightly@latest",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "shiki": "^3.19.0",
    "streamdown": "^1.6.10",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.6",
    "tokenlens": "^1.3.1",
    "tw-animate-css": "^1.3.6",
    "use-stick-to-bottom": "^1.1.1",
    "vite-tsconfig-paths": "^5.1.4",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@netko/typescript-config": "workspace:*",
    "@tailwindcss/postcss": "^4",
    "@tanstack/devtools-event-client": "^0.4.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^5.0.4",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vite": "^7.1.7",
    "@types/bun": "^1.3.4"
  }
}
